{
  "default": {
    "paths": ["./features/**/*.feature"],
    "dryRun": false,
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "require": ["./features_steps/**/*.steps.ts", "./support/hooks.ts"],
    "requireModule": ["ts-node/register"],
    "format": [
      "html:./cucumber-report/cucumber-report.html",
      "progress-bar",
      "json:./test-result/test-results.json",
      "rerun:@rerun.txt"
    ],
    "parallel": 2,
    "retry": 2
  },
  "rerun": {
    "dryRun": false,
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "require": ["./features_steps/**/*.steps.ts", "./support/hooks.ts"],
    "requireModule": ["ts-node/register"],
    "format": [
      "html:./cucumber-report/cucumber-report.html",
      "progress-bar",
      "json:./test-result/test-results.json",
      "rerun:@rerun.txt"
    ],
    "parallel": 2
  }
}
